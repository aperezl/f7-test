<template name="Layout">
  <div class="views tabs">


    <div class="panel-overlay"></div>
    <div class="panel panel-left panel-cover">



      <div class="view view-left navbar-through" data-page="index">
        {{> NavBar}}
        <div class="pages">
          <div class="page" data-page="index">
            <div class="toolbar tabbar tabbar-labels">
                <div class="toolbar-inner">
                    <a href="#homeView" class="tab-link active">
                        <i class="icon fa fa-lg fa-home"></i>
                        <span class="tabbar-label i18n" data-i18n="index.tweet">Home</span>
                    </a>

                    <a href="#view-1" class="tab-link">
                        <i class="icon fa fa-lg fa-users"></i>
                        <span class="tabbar-label i18n" data-i18n="index.contacts">
                          Groups

                        </span>
                    </a>

                    <a href="#settingView" class="tab-link">
                        <i class="icon fa fa-lg fa-cog">
                            <span class="badge badge-red">4</span>
                        </i>
                        <span class="tabbar-label i18n" data-i18n="index.setting">Setting</span>
                    </a>
                </div>
            </div>
            <div class="page-content">
              {{> ListGroups}}
            </div>
          </div>


        </div>




      </div>
    </div>

    <div class="view view-main navbar-through" data-page="item">
      <div class="navbar">
        <div class="navbar-inner navbar-on-center">
          <div class="left sliding">
            <!-- Panel link for split view -->
            <a href="#" class="open-panel link"> open</a>
            <!-- Back link for single view -->
            <a href="#" class="back link"><i class="icon fa fa-lg fa-comments" style="color: #fff"></i></a>
          </div>
          <div class="center sliding">Leaving the Nest</div>
          <div class="right sliding">
            <!-- User link for small screen -->
            <a href="#" data-panel="right" class="open-panel link">
              <i class="icon fa fa-lg fa-bars" style="color: #fff"></i>
            </a>
          </div>
        </div>
      </div>

      <div class="pages">
        <div class="page page-on-center toolbar-fixed" data-page="item">
          <!-- messagebar -->
          <div class="toolbar toolbar-bottom messagebar">
            <div class="toolbar-inner">
              <textarea placeholder="Message"></textarea><a href="#" class="link">Send</a>
            </div>
          </div>

          <div class="page-content">
            {{>yield}}
          </div>
        </div>
      </div>
    </div>

  </div>
</template>



